{
    "containerDefinitions": [
        {
            "name": "<project>-<env>-<task_role>-container",
            "image": "<ECR_REPOSITORY_URL>",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "logConfiguration": {
                "logDriver": "awsfirelens",
                "options": {}
            },
            "essential": true,
            "environment": [],
            "mountPoints": [],
            "volumesFrom": []
        },
        {
            "name": "<project>-<env>-firelens-container",
            "image": "<ECR_REPOSITORY_FIRELENS_URL>",
            "cpu": 0,
            "portMappings": [],
            "essential": true,
            "environment": [
                {
                    "name": "aws_fluent_bit_init_s3_1",
                    "value": "<logging_bucket_arn>/extra.conf"
                },
                {
                    "name": "project",
                    "value": "<project>"
                },
                {
                    "name": "env",
                    "value": "<env>"
                },
                {
                    "name": "task_role",
                    "value": "<task_role>"
                },
                {
                    "name": "REGION",
                    "value": "<REGION>"
                },
                {
                    "name": "logging_bucket_name",
                    "value": "<logging_bucket_name>"
                }
            ],
            "logConfiguration": {
                "logDriver": "awsfirelens",
                "options": {
                    "awslogs-group": "/ecs/<project>/<env>/firelens",
                    "awslogs-region": "ap-northeast-1",
                    "awslogs-stream-prefix": "firelens"
                }
            },
            "mountPoints": [],
            "volumesFrom": []
        }
    ],
    "family": "<project>-<env>-<task_role>-task",
    "executionRoleArn": "<execution_role_arn>",
    "networkMode": "awsvpc",
    "revision": 5,
    "volumes": [],
    "status": "ACTIVE",
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}